generator client {
  provider = "prisma-client-js"
  output   = "./generated"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model web_index {
  id        Int                    @id @default(autoincrement())
  vector_id String                 @unique
  embedding Unsupported("vector")?
  metadata  Json?                  @default("{}")

  @@index([embedding], map: "web_index_vector_idx")
}

model segment {
  id          String   @id
  content     String
  chunks_id   String[]
  node_set    String[]
  dataset_name String
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt
  batch String
}